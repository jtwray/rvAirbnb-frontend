    "axios": "0.21.1",
    "craco-less": "1.17.1",
    "date-fns": "^2.16.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.14.0",
    "react-map-gl": "^6.0.2",
    "react-nice-dates": "^3.0.7",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
